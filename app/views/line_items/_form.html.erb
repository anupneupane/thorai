<%= form_for @line_item do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :deal_id %><br />
    <%= f.text_field :deal_id %>
  </p>
  <p>
    <%= f.label :chest_id %><br />
    <%= f.text_field :chest_id %>
  </p>
  <p>
    <%= f.label :order_id %><br />
    <%= f.text_field :order_id %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p><%= f.submit %></p>
<% end %>
